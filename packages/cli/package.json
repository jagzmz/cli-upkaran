{
  "name": "@cli-upkaran/cli",
  "version": "0.0.2-latest.5",
  "description": "Main CLI entry point for the cli-upkaran toolkit",
  "bin": {
    "cli-upkaran": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && rolldown -c",
    "build:types": "tsc -b",
    "build:bundle": "rolldown -c",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts",
    "start": "node ./dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "echo \"No tests in cli yet\""
  },
  "keywords": [
    "cli-upkaran",
    "cli"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@cli-upkaran/core": "workspace:*",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.0.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.30",
    "@types/ora": "^3.2.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.3",
    "typescript": "^5.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}